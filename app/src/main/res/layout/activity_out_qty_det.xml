<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activity.OutQtyDetActivity">

    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbar"
        android:minHeight="?attr/actionBarSize"
        android:layout_width="match_parent"
        android:background="@color/blue_A700"
        android:layout_height="64dp">
    </android.support.v7.widget.Toolbar>

    <LinearLayout
        android:id="@+id/tvlinetext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/toolbar"
        android:layout_marginTop="10dp"
        android:orientation="horizontal">

<TextView
    android:id="@+id/tvtext"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginLeft="10dp"
    android:text="Input Quantity : "/>



        <TextView
            android:id="@+id/tvinqty3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:textStyle="bold"
            android:text="In Qty"/>

        <TextView
            android:id="@+id/tvqcname2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:text="qcname"/>

        <TextView
            android:id="@+id/tvusername6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:text="username"/>

        <TextView
            android:id="@+id/tvposted5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/white"
            android:layout_marginLeft="5dp"
            android:text="ini posted"/>

        <TextView
            android:id="@+id/tvstatus1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="in i status"
            android:textColor="@color/white"
            android:layout_marginLeft="5dp"/>
        <TextView
            android:id="@+id/tvjammulai0"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="6dp"
            android:textColor="@color/white"
            android:text="jammulai"/>
        <TextView
            android:id="@+id/tvtglmulai0"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:textColor="@color/white"
            android:text="tgl mulai"/>
        <TextView
            android:id="@+id/tvroutename0"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:textColor="@color/white"
            android:text="route name"/>
        <TextView
            android:id="@+id/tvroutecode0"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:textColor="@color/white"
            android:text="toutecode"/>
        <TextView
            android:id="@+id/tvprodplanqty1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:textColor="@color/white"
            android:text="prodplanqtty"/>
        <TextView
            android:id="@+id/tvprodstatus0"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/white"
            android:text="prodstatus"/>
        <TextView
            android:id="@+id/tvprodcode1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:textColor="@color/white"
            android:text="prodcode"/>
        <TextView
            android:id="@+id/tvworkcenter1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:textColor="@color/white"
            android:text="ini workcenter"/>
        <TextView
            android:id="@+id/tvdocentry2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/white"
            android:text="ini docentry"/>
        <TextView
            android:id="@+id/tvdocnum6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="docnum"
            android:textColor="@color/white"/>
        <TextView
            android:id="@+id/tvnoprod3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:textColor="@color/white"
            android:text="noprod"/>

        <TextView
            android:id="@+id/tvsequence3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:textColor="@color/white"
            android:text="sequence"/>

        <TextView
            android:id="@+id/tvseqqty2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:textColor="@color/white"
            android:text="seq qty"/>
        <TextView
            android:id="@+id/tvnmprod2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:textColor="@color/white"
            android:text="nama produksi"/>



    </LinearLayout>

    <LinearLayout
        android:id="@+id/tvlineinseq"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/toolbar"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tvOutputSeq1"
            android:layout_width="400dp"
            android:layout_height="350dp"
            android:hint="Out Qty"
            android:textSize="60sp"
            android:layout_gravity="center"
            android:gravity="center"/>
    </LinearLayout>


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="15dp"
        android:layout_below="@id/toolbar"
        android:layout_toRightOf="@id/tvlineinseq"
        android:layout_marginTop="10dp">

        <Button
            android:id="@+id/btn1"
            android:layout_width="100dp"
            android:layout_height="90dp"
            android:text="1"
            android:onClick="btn1Clicked"
            android:textSize="60sp"/>
        <Button
            android:id="@+id/btn2"
            android:layout_width="100dp"
            android:layout_height="90dp"
            android:layout_toRightOf="@id/btn1"
            android:text="2"
            android:onClick="btn2Clicked"
            android:textSize="60sp"/>
        <Button
            android:id="@+id/btn3"
            android:layout_width="100dp"
            android:layout_height="90dp"
            android:layout_toRightOf="@id/btn2"
            android:text="3"
            android:onClick="btn3Clicked"
            android:textSize="60sp"/>
        <Button
            android:id="@+id/btn4"
            android:layout_width="100dp"
            android:layout_height="90dp"
            android:layout_below="@id/btn1"
            android:text="4"
            android:onClick="btn4Clicked"
            android:textSize="60sp"/>
        <Button
            android:id="@+id/btn5"
            android:layout_width="100dp"
            android:layout_height="90dp"
            android:layout_below="@id/btn2"
            android:layout_toRightOf="@id/btn4"
            android:text="5"
            android:onClick="btn5Clicked"
            android:textSize="60sp"/>
        <Button
            android:id="@+id/btn6"
            android:layout_width="100dp"
            android:layout_height="90dp"
            android:layout_below="@id/btn3"
            android:layout_toRightOf="@id/btn5"
            android:text="6"
            android:onClick="btn6Clicked"
            android:textSize="60sp"/>
        <Button
            android:id="@+id/btn7"
            android:layout_width="100dp"
            android:layout_height="90dp"
            android:layout_below="@id/btn4"
            android:text="7"
            android:onClick="btn7Clicked"
            android:textSize="60sp"/>
        <Button
            android:id="@+id/btn8"
            android:layout_width="100dp"
            android:layout_height="90dp"
            android:layout_below="@id/btn5"
            android:layout_toRightOf="@id/btn7"
            android:text="8"
            android:onClick="btn8Clicked"
            android:textSize="60sp"/>
        <Button
            android:id="@+id/btn9"
            android:layout_width="100dp"
            android:layout_height="90dp"
            android:layout_below="@id/btn6"
            android:layout_toRightOf="@id/btn8"
            android:text="9"
            android:onClick="btn9Clicked"
            android:textSize="60sp"/>
        <Button
            android:id="@+id/btnClear"
            android:layout_width="100dp"
            android:layout_height="90dp"
            android:layout_below="@id/btn7"
            android:text="Clear"
            android:onClick="btnclearClicked"
            android:textSize="20sp"/>
        <Button
            android:id="@+id/btn0"
            android:layout_width="100dp"
            android:layout_height="90dp"
            android:layout_below="@id/btn8"
            android:layout_toRightOf="@id/btnClear"
            android:text="0"
            android:onClick="btn0Clicked"
            android:textSize="60sp"/>
        <Button
            android:id="@+id/btnBack"
            android:layout_width="100dp"
            android:layout_height="90dp"
            android:layout_below="@id/btn9"
            android:layout_toRightOf="@id/btn0"
            android:text="Back"
            android:textSize="20sp"/>
    </RelativeLayout>

</RelativeLayout>

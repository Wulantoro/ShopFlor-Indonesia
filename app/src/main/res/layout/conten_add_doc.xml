<!--                android:text="wcname"/>-->

            <TextView
                android:id="@+id/tvip4"
                android:layout_width="45dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:text="ip server"
                android:textColor="@color/white" />

            <TextView
                android:id="@+id/tvblnkmrn"
                android:layout_width="wrap_content"

                android:layout_height="wrap_content"
                android:layout_below="@id/tvSquence_Qty"
                android:layout_marginTop="20dp"
                android:layout_marginLeft="20dp"
                android:textSize="@dimen/_11font_mdp"
                android:layout_toRightOf="@id/tvShift"

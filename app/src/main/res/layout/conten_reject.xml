<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    xmlns:tools="http://schemas.android.com/tools"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:showIn="@layout/activity_reject">


    <LinearLayout
        android:id="@+id/dashboard"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:orientation="vertical"
        android:layout_marginLeft="10dp">

        <TextView
            android:id="@+id/tvnoprod"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="No. Produksi : "/>

        <TextView
            android:id="@+id/tvnoprod1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="ini no produksi"/>

        <TextView
            android:id="@+id/tvnmprod"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="Nama Produk"/>
        <TextView
            android:id="@+id/tvnmprod1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="ini nama produk"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="Sequence"/>
        <TextView
            android:id="@+id/tvsequence1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="ini sequence"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="Sequence Qty"/>
        <TextView
            android:id="@+id/tvseqqty1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="ini sequence qty"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="Tanggal Selesai :"/>
        <TextView
            android:id="@+id/tvtglsel1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="iin tanggal selesai"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="Jam Selesai"/>
        <TextView
            android:id="@+id/tvjamsel1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="ini jam selesai"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="Doc.Status"/>
        <TextView
            android:id="@+id/tvdocsts2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="ini doc status"
            />
        </LinearLayout>

    <LinearLayout
        android:id="@+id/RootContent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="10dp"
        android:orientation="horizontal"
        android:layout_toRightOf="@id/dashboard">

        <RelativeLayout
            android:id="@+id/content1"
            android:layout_width="150dp"
            android:layout_height="70dp">

            <TextView
                android:id="@+id/tvInputQty"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Input Quantity" />

            <TextView
                android:id="@+id/tvInputQty1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/tvInputQty"
                android:text="ini input quantity" />
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/content2"
            android:layout_width="150dp"
            android:layout_height="70dp"
            >
            <TextView
                android:id="@+id/tvOutputQty"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Output Quantity"/>
            <TextView
                android:id="@+id/tvOutputQty1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/tvOutputQty"
                android:text="ini output quantity"/>

        </RelativeLayout>

        <TextView
            android:id="@+id/tvWorkcenter"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@id/tvOutputQty"
            android:text="workcemter"/>


    </LinearLayout>
    <RelativeLayout
        android:id="@+id/tvtampil"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_below="@id/RootContent"
        android:layout_toRightOf="@id/dashboard"
        android:layout_marginLeft="20dp">

        <TextView
            android:id="@+id/tvdocentry0"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="docentryyyyyyyy"/>
        <TextView
            android:id="@+id/tvdocnum1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@id/tvdocentry0"
            android:layout_marginLeft="5dp"
            android:text="docnum"/>
        <TextView
            android:id="@+id/tvprodcode0"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@id/tvdocnum1"
            android:layout_marginLeft="5dp"
            android:text="prodcode"/>
        <TextView
            android:id="@+id/tvprodplanqty0"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@id/tvprodcode0"
            android:layout_marginLeft="5dp"
            android:text="prodplanqty"/>
        <TextView
            android:id="@+id/tvprodstatus2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@id/tvprodplanqty0"
            android:layout_marginLeft="5dp"
            android:text="prod status"/>
        <TextView
            android:id="@+id/tvroutecode2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@id/tvprodstatus2"
            android:layout_marginLeft="5dp"
            android:text="route codecode"/>
        <TextView
            android:id="@+id/tvroutename2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@id/tvroutecode2"
            android:layout_marginLeft="5dp"
            android:text="routenmae"/>
        <TextView
            android:id="@+id/tvdocdate0"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@id/tvroutename2"
            android:layout_marginLeft="5dp"
            android:text="docdate"/>
        <TextView
            android:id="@+id/tvjammulai2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/tvdocentry0"
            android:text="jam mulai"/>
        <TextView
            android:id="@+id/tvworkcenter6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/tvdocentry0"
            android:layout_toRightOf="@id/tvjammulai2"
            android:layout_marginLeft="5dp"
            android:text="workcenter"/>
        <TextView
            android:id="@+id/tvstatus0"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/tvdocentry0"
            android:layout_toRightOf="@id/tvworkcenter6"
            android:layout_marginLeft="5dp"
            android:text="status"/>
        <TextView
            android:id="@+id/tvposted7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/tvdocentry0"
            android:layout_toRightOf="@id/tvstatus0"
            android:layout_marginLeft="5dp"
            android:text="posted"/>
        <TextView
            android:id="@+id/tvusername8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/tvdocentry0"
            android:layout_toRightOf="@id/tvposted7"
            android:layout_marginLeft="5dp"
            android:text="username"/>
        <TextView
            android:id="@+id/tvqcname4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/tvdocentry0"
            android:layout_toRightOf="@id/tvusername8"
            android:layout_marginLeft="5dp"
            android:text="qcname"/>

        <Button
            android:id="@+id/btnFrag"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/tvjammulai2"
            android:text="Tambah Reject"/>

        <android.support.v7.widget.RecyclerView
            android:id="@+id/rvInputReject"
            android:layout_below="@id/btnFrag"
            android:layout_width="wrap_content"
            android:layout_marginTop="10dp"
            android:layout_height="wrap_content"
            tools:listitem="@layout/item_reject_row">
        </android.support.v7.widget.RecyclerView>
    </RelativeLayout>

</RelativeLayout>
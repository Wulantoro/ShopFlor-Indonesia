<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".Activity.AddSeq1Activity"
    android:orientation="horizontal"
    tools:showIn="@layout/activity_add_seq1">


  <LinearLayout
      android:id="@+id/tvlineinseq"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:orientation="horizontal">

    <TextView
        android:id="@+id/tvInputSeq1"
        android:layout_width="@dimen/_210mdp"
        android:layout_height="@dimen/_230mdp"
        android:hint="Out Qty"
        android:textSize="60sp"
        android:layout_gravity="center"
        android:gravity="center"/>
  </LinearLayout>


  <RelativeLayout
      android:layout_width="match_parent"
      android:layout_height="match_parent"
      android:layout_marginLeft="@dimen/_20mdp"
      android:layout_toRightOf="@id/tvlineinseq"
      android:layout_marginTop="@dimen/_15mdp">

    <Button
        android:id="@+id/btn1"
        android:layout_width="@dimen/_80mdp"
        android:layout_height="@dimen/_55mdp"
        android:text="1"
        android:onClick="btn1Clicked"
        android:background="@drawable/button"
        android:textSize="@dimen/_35font_mdp"/>
    <Button
        android:id="@+id/btn2"
        android:layout_width="@dimen/_80mdp"
        android:layout_height="@dimen/_55mdp"
        android:layout_toRightOf="@id/btn1"
        android:background="@drawable/button"
        android:text="2"
        android:onClick="btn2Clicked"
        android:textSize="@dimen/_35font_mdp"/>
    <Button
        android:id="@+id/btn3"
        android:layout_width="@dimen/_80mdp"
        android:layout_height="@dimen/_55mdp"
        android:layout_toRightOf="@id/btn2"
        android:background="@drawable/button"
        android:text="3"
        android:onClick="btn3Clicked"
        android:textSize="@dimen/_35font_mdp"/>
    <Button
        android:id="@+id/btn4"
        android:layout_width="@dimen/_80mdp"
        android:layout_height="@dimen/_55mdp"
        android:layout_below="@id/btn1"
        android:background="@drawable/button"
        android:text="4"
        android:onClick="btn4Clicked"
        android:textSize="@dimen/_35font_mdp"/>
    <Button
        android:id="@+id/btn5"
        android:layout_width="@dimen/_80mdp"
        android:layout_height="@dimen/_55mdp"
        android:layout_below="@id/btn2"
        android:layout_toRightOf="@id/btn4"
        android:background="@drawable/button"
        android:text="5"
        android:onClick="btn5Clicked"
        android:textSize="@dimen/_35font_mdp"/>
    <Button
        android:id="@+id/btn6"
        android:layout_width="@dimen/_80mdp"
        android:layout_height="@dimen/_55mdp"
        android:layout_below="@id/btn3"
        android:layout_toRightOf="@id/btn5"
        android:background="@drawable/button"
        android:text="6"
        android:onClick="btn6Clicked"
        android:textSize="@dimen/_35font_mdp"/>
    <Button
        android:id="@+id/btn7"
        android:layout_width="@dimen/_80mdp"
        android:layout_height="@dimen/_55mdp"
        android:layout_below="@id/btn4"
        android:background="@drawable/button"
        android:text="7"
        android:onClick="btn7Clicked"
        android:textSize="@dimen/_35font_mdp"/>
    <Button
        android:id="@+id/btn8"
        android:layout_width="@dimen/_80mdp"
        android:layout_height="@dimen/_55mdp"
        android:layout_below="@id/btn5"
        android:layout_toRightOf="@id/btn7"
        android:background="@drawable/button"
        android:text="8"
        android:onClick="btn8Clicked"
        android:textSize="@dimen/_35font_mdp"/>
    <Button
        android:id="@+id/btn9"
        android:layout_width="@dimen/_80mdp"
        android:layout_height="@dimen/_55mdp"
        android:layout_below="@id/btn6"
        android:layout_toRightOf="@id/btn8"
        android:background="@drawable/button"
        android:text="9"
        android:onClick="btn9Clicked"
        android:textSize="@dimen/_35font_mdp"/>
    <Button
        android:id="@+id/btnClear"
        android:layout_width="@dimen/_80mdp"
        android:layout_height="@dimen/_55mdp"
        android:layout_below="@id/btn7"
        android:background="@drawable/button"
        android:text="Clear"
        android:onClick="btnclearClicked"
        android:textSize="20sp"/>
    <Button
        android:id="@+id/btn0"
        android:layout_width="@dimen/_80mdp"
        android:layout_height="@dimen/_55mdp"
        android:layout_below="@id/btn8"
        android:background="@drawable/button"
        android:layout_toRightOf="@id/btnClear"
        android:text="0"
        android:onClick="btn0Clicked"
        android:textSize="@dimen/_35font_mdp"/>
    <Button
        android:id="@+id/btnBack"
        android:layout_width="@dimen/_80mdp"
        android:layout_height="@dimen/_55mdp"
        android:layout_below="@id/btn9"
        android:background="@drawable/button"
        android:layout_toRightOf="@id/btn0"
        android:text="Back"
        android:textSize="20sp"/>
  </RelativeLayout>

</RelativeLayout>
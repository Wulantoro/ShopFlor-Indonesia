<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Fragment.RejectListFragment">


    <RelativeLayout
        android:id="@+id/lineReject"
        android:layout_width="350dp"
        android:layout_height="350dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="20dp"
        android:orientation="vertical">

    <TextView
        android:id="@+id/tvReject2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:layout_marginTop="20dp"
        android:text="Choose Reject : "
        android:gravity="center_horizontal"
        />

        <TextView
            android:id="@+id/tvdocentry4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@id/tvReject2"
            android:textColor="@color/white"
            android:text="docentryyyyy"/>

        <TextView
            android:id="@+id/tvlinenumb0"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@id/tvdocentry4"
            android:layout_marginLeft="5dp"
            android:textColor="@color/white"
            android:text="line numb"/>

        <TextView
            android:id="@+id/tvid6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@id/tvlinenumb0"
            android:layout_marginLeft="5dp"
            android:textColor="@color/white"
            android:text="id"/>

        <TextView
            android:id="@+id/tvmobileid1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@id/tvid6"
            android:layout_marginLeft="5dp"
            android:textColor="@color/white"
            android:text="mobileid"/>

        <TextView
            android:id="@+id/tvip14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:layout_toRightOf="@id/tvmobileid1"
            android:textColor="@color/white"
            android:text="ip server"/>

        <TextView
            android:id="@+id/tvcodereject0"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/tvReject2"
            android:layout_marginTop="10dp"
            android:textSize="20sp"
            android:hint="Code Reject"/>

        <TextView
            android:id="@+id/tvReject3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_below="@id/tvReject2"
            android:layout_toRightOf="@id/tvcodereject0"
            android:layout_marginLeft="10dp"
            android:text="Reject"
            android:layout_marginTop="10dp"
            android:textSize="20sp"
            />


        <Button
            android:id="@+id/btnReject"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Pilih Reject"
            android:gravity="center_horizontal"
            android:layout_below="@id/tvReject3"
            android:layout_gravity="center_horizontal"
            android:background="@drawable/button"
            android:layout_marginTop="10dp"/>

        <Button
            android:id="@+id/btnSimpan"
            android:layout_width="350dp"
            android:layout_height="match_parent"
            android:text="SIMPAN"
            android:textSize="20sp"
            android:layout_below="@id/btnReject"
            android:background="@drawable/button"
            android:layout_marginTop="20dp"
            />


    </RelativeLayout>

    <TextView
        android:id="@+id/tvtotreject1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/lineReject"
        android:textColor="@color/white"
        android:text="total reject"/>

    <TextView
        android:id="@+id/tvinqty1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@id/tvtotreject1"
        android:layout_marginLeft="5dp"
        android:text="in qqty"
        android:textColor="@color/white"
        android:layout_below="@id/lineReject"/>

    <TextView
        android:id="@+id/tvoutqty1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@id/tvinqty1"
        android:layout_below="@id/lineReject"
        android:layout_marginLeft="5dp"
        android:textColor="@color/white"
        android:text="out qty"/>

    <TextView
        android:id="@+id/tvgrantot"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@id/tvoutqty1"
        android:layout_marginLeft="5dp"
        android:layout_below="@id/lineReject"
        android:textColor="@color/white"
        android:text="grantot"/>

    <RelativeLayout
        android:id="@+id/relinputRej"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="4dp"
        android:layout_toRightOf="@id/lineReject"
        android:layout_marginTop="20dp"
        >

        <TextView
            android:id="@+id/tvRejectQty"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginTop="10dp"
            android:gravity="center"
            android:hint="Reject Qty"
            android:textSize="30sp" />

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/tvRejectQty"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp">

            <Button
                android:id="@+id/btn1"
                android:layout_width="100dp"
                android:layout_height="70dp"
                android:background="@drawable/button"
                android:onClick="btn1Clicked"
                android:text="1"
                android:textSize="40sp" />

            <Button
                android:id="@+id/btn2"
                android:layout_width="100dp"
                android:layout_height="70dp"
                android:layout_toRightOf="@id/btn1"
                android:background="@drawable/button"
                android:onClick="btn2Clicked"
                android:text="2"
                android:textSize="40sp" />

            <Button
                android:id="@+id/btn3"
                android:layout_width="100dp"
                android:layout_height="70dp"
                android:layout_toRightOf="@id/btn2"
                android:background="@drawable/button"
                android:onClick="btn3Clicked"
                android:text="3"
                android:textSize="40sp" />

            <Button
                android:id="@+id/btn4"
                android:layout_width="100dp"
                android:layout_height="70dp"
                android:layout_below="@id/btn1"
                android:background="@drawable/button"
                android:onClick="btn4Clicked"
                android:text="4"
                android:textSize="40sp" />

            <Button
                android:id="@+id/btn5"
                android:layout_width="100dp"
                android:layout_height="70dp"
                android:layout_below="@id/btn2"
                android:layout_toRightOf="@id/btn4"
                android:background="@drawable/button"
                android:onClick="btn5Clicked"
                android:text="5"
                android:textSize="40sp" />

            <Button
                android:id="@+id/btn6"
                android:layout_width="100dp"
                android:layout_height="70dp"
                android:layout_below="@id/btn3"
                android:layout_toRightOf="@id/btn5"
                android:background="@drawable/button"
                android:onClick="btn6Clicked"
                android:text="6"
                android:textSize="40sp" />

            <Button
                android:id="@+id/btn7"
                android:layout_width="100dp"
                android:layout_height="70dp"
                android:layout_below="@id/btn4"
                android:background="@drawable/button"
                android:onClick="btn7Clicked"
                android:text="7"
                android:textSize="40sp" />

            <Button
                android:id="@+id/btn8"
                android:layout_width="100dp"
                android:layout_height="70dp"
                android:layout_below="@id/btn5"
                android:layout_toRightOf="@id/btn7"
                android:background="@drawable/button"
                android:onClick="btn8Clicked"
                android:text="8"
                android:textSize="40sp" />

            <Button
                android:id="@+id/btn9"
                android:layout_width="100dp"
                android:layout_height="70dp"
                android:layout_below="@id/btn6"
                android:layout_toRightOf="@id/btn8"
                android:background="@drawable/button"
                android:onClick="btn9Clicked"
                android:text="9"
                android:textSize="40sp" />

            <Button
                android:id="@+id/btnClear"
                android:layout_width="100dp"
                android:layout_height="70dp"
                android:layout_below="@id/btn7"
                android:background="@drawable/button"
                android:onClick="btnClearClic"
                android:text="Clear"
                android:textSize="20sp" />

            <Button
                android:id="@+id/btn0"
                android:layout_width="100dp"
                android:layout_height="70dp"
                android:layout_below="@id/btn8"
                android:layout_toRightOf="@id/btnClear"
                android:background="@drawable/button"
                android:onClick="btn0Click"
                android:text="0"
                android:textSize="40sp" />

            <Button
                android:id="@+id/btnBack"
                android:layout_width="100dp"
                android:layout_height="70dp"
                android:layout_below="@id/btn9"
                android:layout_toRightOf="@id/btn0"
                android:background="@drawable/button"
                android:text="Back"
                android:textSize="20sp" />
        </RelativeLayout>
    </RelativeLayout>

</RelativeLayout>